<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="102.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.audioplayer.AskrifandiDialog">
    <TextField fx:id="fxNafnField" promptText="Nafn" />
   <PasswordField fx:id="fxLykilorð" promptText="Lykiloð" />
    <Button fx:id="fxLoginButton" mnemonicParsing="false" onAction="#onLogin" text="Skrá inn" />
    <RadioButton fx:id="fxIslenska3" mnemonicParsing="false" onAction="#onIslenskaClicked3" selected="true" text="Íslenska" />
    <RadioButton fx:id="fxEnska3" mnemonicParsing="false" onAction="#onEnskaClicked3" text="Enska" />
</VBox>
